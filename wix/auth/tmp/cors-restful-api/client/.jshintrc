{
  "node": true,
  "browser": true,
  "es5": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": false,
  "immed": true,
  "indent": 4,
  "latedef": false,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": false,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "laxcomma"      : true,
  "laxbreak"      : true,
  "browser"       : true,
  "maxlen"        : 120,
  "predef"        : [
      "app",
      "window",
      "document",
      "console",
      "angular",
      "_",
      "md5",
      "module",
      "describe",
      "beforeEach",
      "it",
      "chai",
      "inject"
  ]
}
